<div class="thumbnails">
    <h1 class="title"><%= title %></h1>
    <div class="row">
<%
    thumbnails.each(function(thumbnail)
    {
%>
        <div class="col-xs-3">
            <a href="<%= thumbnail.attr('ref') %>" class="thumbnail">
            <img src="<%= thumbnail.attr('src') %>" alt="..." />
            </a>
        </div>
<%
    });
%>        
    </div>
</div>